 process{
    withName: 'ANNOTATE_DRIVER' {
        publishDir = [
                [
                    mode: params.publish_dir_mode,
                    path: { "${params.outdir}/DriverAnnotation/${meta.dataset}/${meta.patient}/${meta.tumour_sample}/"},
                    pattern: "*{rds}"
                ]
            ]    
        memory = '10 GB'
        time = '1h'
        errorStrategy = 'finish'
    }
}