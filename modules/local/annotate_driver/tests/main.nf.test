nextflow_process {
    name 'Test process ANNOTATE_DRIVER'
    tag 'modules'

    script '../main.nf'
    process 'ANNOTATE_DRIVER'

    // FIXME VCF.rds should be readed from raw.github
    test("Should run without failures") {
        when {
            process {
                """
                input[0] = [
                    'testDatasetId',
                    'patientID',
                    'Set6_42',
                    file("${projectDir}/VCF.rds", checkIfExists: true)
                ]

                input[1] = 'PANCANCER'
                """
            }
        }

        then {
            assertAll(
                { assert process.success }
            )
        }
    }
}